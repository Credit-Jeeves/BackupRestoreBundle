{
    "name": "comfortablynumb/backup-restore-bundle",
    "type": "symfony-bundle",
    "description": "Backup and Restore helper classes to run DB vendor's utilities for such tasks",
    "license": "MIT",
    "authors": [
        {
            "name": "Gustavo Falco"
        }
    ],

    "require": {
        "php": ">=5.3.3",
        "ext-mcrypt": "*",
        "doctrine/dbal": ">=2.0",
        "symfony/framework-bundle": "6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },

    "autoload": {
        "psr-4": { "ENC\\Bundle\\BackupRestoreBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "ENC\\Bundle\\BackupRestoreBundle\\Tests\\": "Tests/" }
    }
}
